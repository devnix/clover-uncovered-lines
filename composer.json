{
  "name": "devnix/clover-uncovered-lines",
  "description": "A tool to extract uncovered lines from Clover code coverage reports.",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Pablo Largo Mohedano",
      "email": "devnix.code@gmail.com"
    }
  ],
  "require": {
    "php": "^8.4"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.48",
    "friendsofphp/php-cs-fixer": "^3.88",
    "infection/infection": "^0.32.2",
    "phpstan/extension-installer": "^1.4",
    "phpstan/phpstan": "^2.1",
    "phpstan/phpstan-deprecation-rules": "^2.0",
    "phpstan/phpstan-phpunit": "^2.0",
    "phpstan/phpstan-strict-rules": "^2.0",
    "phpunit/phpunit": "^12.4",
    "rector/rector": "^2.2",
    "symfony/var-dumper": "^7.3",
    "symplify/phpstan-rules": "^14.8"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Devnix\\CloverUncoveredLines\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Devnix\\CloverUncoveredLines\\Tests\\": "tests/"
    }
  },
  "bin": [
    "bin/clover-uncovered-lines"
  ],
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true,
      "infection/extension-installer": true,
      "phpstan/extension-installer": true
    }
  }
}
